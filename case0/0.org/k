FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1666;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.1666;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.1666;
        value           uniform 0.1666;
    }
    ground
    {
        //type            kqRWallFunction;
        //value           uniform 0.1666;
        type            symmetry;
    }
    top
    {
        type            symmetry;
    }
    "side.*"
    {
        type            symmetry;
    }
    BW
    {
        type            fixedValue;
        value           uniform 0.1666;
    }
    FLWh
    {
        type            fixedValue;
        value           uniform 0.1666;
    }
    FW
    {
        type            fixedValue;
        value           uniform 0.1666;
    }
}

