forceCoeffs1
{
    type            forceCoeffs;

    libs            ("libforces.so");

    writeControl    timeStep;
    timeInterval    1;

    log             yes;

    patches         (BW FW FLWh);
    rho             rhoInf;      // Indicates incompressible
    rhoInf          1;           // Redundant for incompressible
    liftDir         (0 0 1);
    dragDir         (1 0 0);
    CofR            (-1.07 0 0);  // 1/4 Chord Point
    pitchAxis       (0 1 0);
    magUInf         40;
    lRef            0.29;        // Wing Chord
    Aref            1.4;        // Wing Planform Area 
    /*
    binData
    {
        nBin        20;          // output data into 20 bins
        direction   (1 0 0);     // bin direction
        cumulative  yes;
    }
    */
}


